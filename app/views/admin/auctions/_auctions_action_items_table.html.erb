<table class="admin-auctions-table">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Delivery Deadline</th>
      <th scope="col">Delivery URL</th>
      <th scope="col">Result</th>
      <th scope="col">CAP URL</th>
      <th scope="col">Paid?</th>
    </tr>
  </thead>
  <tbody>
    <% auctions.each do |auction| %>
    <tr>
      <td>
        <%= link_to(auction.title, admin_auction_path(auction)) %>
        (<%= link_to('edit', edit_admin_auction_path(auction)) %>)
      </td>
      <td><%= auction.delivery_deadline %></td>
      <td><%= auction.delivery_url %></td>
      <td><%= auction.result %></td>
      <td><%= auction.cap_proposal_url %></td>
      <td><%= auction.paid? %></td>
    </tr>
    <% end %>
  </tbody>
</table>
